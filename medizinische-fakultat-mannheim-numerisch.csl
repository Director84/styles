<?xml version="1.0" encoding="utf-8"?>
<style class="in-text" version="1.0" demote-non-dropping-particle="sort-only" default-locale="de-DE" xmlns="http://purl.org/net/xbiblio/csl">
  <!-- This style was edited with the Visual CSL Editor (https://editor.citationstyles.org/visualEditor/) -->
  <info>
    <title>Medizinische Fakultät Mannheim - Numerisch</title>
    <title-short>MedMa</title-short>
    <id>http://www.zotero.org/styles/medizinische-fakultat-mannheim-numerisch</id>
    <link href="http://www.zotero.org/styles/medizinische-fakultat-mannheim-numerisch" rel="self"/>
    <link href="http://www.zotero.org/styles/journal-of-the-american-society-of-nephrology" rel="template"/>
    <link href="https://www.umm.uni-heidelberg.de/fakultaet/promotion/promotion-dr-med/" rel="documentation"/>
    <author>
      <name>Norman Roscher</name>
      <email>n.roscher@stud.uni-heidelberg.de</email>
    </author>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <summary>Based on &quot;Journal of the American Society of Nephrology&quot; - adapted for: Medizinische Fakultät Mannheim der Universität Heidelberg</summary>
    <updated>2019-12-29T03:31:20+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names variable="author" delimiter="," suffix=": ">
      <name delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all"/>
      <label form="long" prefix=", "/>
      <substitute>
        <names variable="editor">
          <substitute>
            <text variable="publisher"/>
          </substitute>
        </names>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <choose>
      <if type="article" match="none" variable="edition">
        <text value="Edited by" suffix=" "/>
      </if>
      <else>
        <text macro="edition" suffix=", "/>
        <text value="edited by" text-case="lowercase" strip-periods="false" suffix=" "/>
      </else>
    </choose>
    <names variable="editor" font-variant="small-caps" vertical-align="baseline" suffix=",">
      <name font-variant="normal" suffix="." delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all"/>
    </names>
  </macro>
  <macro name="publisher">
    <text variable="publisher-place" suffix=", "/>
    <text variable="publisher" suffix=","/>
  </macro>
  <macro name="access">
    <group>
      <text value="Online: "/>
      <text variable="URL" suffix=","/>
    </group>
  </macro>
  <macro name="accessed-date">
    <choose>
      <if variable="URL">
        <group delimiter=" ">
          <text value="Stand:"/>
          <date form="numeric" variable="accessed" suffix="."/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="journal-title">
    <choose>
      <if type="article-journal article-magazine" match="any">
        <group suffix=" ">
          <text variable="container-title" form="short" font-style="italic" suffix=","/>
        </group>
      </if>
      <else-if type="webpage" match="any">
        <choose>
          <if match="any" variable="note">
            <text variable="note" suffix=". "/>
          </if>
          <else-if match="any" variable="container-title">
            <text variable="container-title" suffix=". "/>
          </else-if>
        </choose>
      </else-if>
      <else>
        <text variable="container-title" suffix=". " form="short"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <group delimiter=" ">
      <choose>
        <if type="webpage" match="any">
          <text variable="title" font-style="italic" suffix=". "/>
          <date date-parts="year" form="text" variable="issued" suffix="."/>
        </if>
        <else>
          <choose>
            <if type="chapter entry entry-dictionary entry-encyclopedia" match="any" variable="container-title container-title-short container-author">
              <text variable="title" suffix="."/>
            </if>
            <else>
              <text variable="title" font-style="italic" suffix="."/>
            </else>
          </choose>
          <choose>
            <if type="thesis" match="any">
              <text value="Med. Dissertation."/>
              <text variable="publisher" suffix=", "/>
            </if>
          </choose>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short" strip-periods="false"/>
        </group>
      </if>
      <else>
        <text variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," vertical-align="sup">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography second-field-align="flush">
    <layout>
      <text variable="citation-number" suffix=". "/>
      <text macro="author"/>
      <text macro="title" suffix=" "/>
      <choose>
        <if type="patent" match="any">
          <group>
            <choose>
              <if match="any" variable="number">
                <text value="Patentnr." prefix=" " suffix=" "/>
                <text variable="number" suffix=", "/>
              </if>
              <else-if match="any" variable="call-number">
                <text value="Bewerbungsnr." suffix=" "/>
                <text variable="call-number" suffix=", "/>
              </else-if>
            </choose>
            <date form="numeric" variable="issued" suffix=". "/>
            <choose>
              <if match="any" variable="status">
                <text value="Status:" suffix=" "/>
                <text variable="status" suffix=". "/>
              </if>
            </choose>
          </group>
        </if>
        <else>
          <choose>
            <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
              <group prefix=" " delimiter=" ">
                <text macro="publisher"/>
                <text macro="edition"/>
                <date date-parts="year" form="text" variable="issued" suffix=".">
                  <date-part name="year"/>
                </date>
              </group>
            </if>
            <else-if type="chapter paper-conference" match="any">
              <group prefix=" " suffix=", ">
                <text term="in" suffix=": " text-case="capitalize-first"/>
                <group delimiter=" ">
                  <text variable="container-title" font-style="italic" suffix="."/>
                  <choose>
                    <if match="any" variable="editor">
                      <text macro="editor"/>
                    </if>
                    <else>
                      <text macro="edition"/>
                    </else>
                  </choose>
                  <text variable="publisher-place" suffix=","/>
                  <text variable="publisher"/>
                </group>
              </group>
              <group delimiter=", ">
                <group>
                  <date variable="issued" suffix=".">
                    <date-part name="year"/>
                  </date>
                </group>
                <group delimiter=" ">
                  <text value="pp"/>
                  <text variable="page" suffix="."/>
                </group>
              </group>
            </else-if>
            <else>
              <text macro="journal-title"/>
              <text variable="volume" suffix=": "/>
              <group delimiter=", ">
                <text variable="page"/>
                <choose>
                  <if type="webpage" match="none">
                    <date variable="issued" suffix=".">
                      <date-part name="year"/>
                    </date>
                  </if>
                </choose>
              </group>
            </else>
          </choose>
        </else>
      </choose>
      <text macro="access" prefix=" "/>
      <text macro="accessed-date" prefix=" "/>
    </layout>
  </bibliography>
</style>
